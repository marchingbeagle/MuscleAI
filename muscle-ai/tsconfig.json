{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@services/*": ["src/services/*"],
      "@assets/*": ["src/assets/*"]
    },
    "esModuleInterop": true,
    "types": ["@prisma/react-native"]
  },
  "include": ["src", "src/types"]
}
